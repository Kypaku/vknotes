{
  "name": "VKNotes",
 
 "description": "VKNotes",
 
 "version": "1.0",
 
  "background": {
    "scripts": ["background.js"]
  },
 
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": [],
      "js": ["jquery-2.1.1.min.js","jquery.mousewheel.js","Underscore.js","gm.js","js.js"]
    }
  ],
 
 "permissions": [
    "tabs","cookies","storage","http://*/*", "https://*/*","webRequest","http://ferz2/*"
  ],
 
 "browser_action": {
    
  "default_title": "Notes for VK dialogs",
    
  "default_icon": "icon.png",
   
   "default_popup": "popup.html"
  },

  "manifest_version": 2
}
